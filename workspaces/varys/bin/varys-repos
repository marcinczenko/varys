#!/usr/bin/env node

'use strict'

const program = require('commander')
const pkg = require('../package.json')

program
  .version(pkg.version)
  .description(pkg.description)
  .command('show [organizations...]', 'shows repositories.')
  .command(
    'protect-branch <owner> <repo>',
    'protects the repository master branch.'
  )
  .parse(process.argv)
