{
  "name": "Varys",
  "version": "0.0.1",
  "description": "Varys",
  "main": "index.js",
  "scripts": {
    "lint": "yarn standard",
    "lint:fix": "yarn standard --fix",
    "test": "jest src --passWithNoTests",
    "coverage": "jest --coverage src",
    "varys": "./src/varys.js"
  },
  "author": "Jeroen Knoops",
  "license": "MIT",
  "bin": {
    "varys": "./src/varys.js"
  },
  "devDependencies": {
    "babel-eslint": "^10.0.1",
    "jest": "^24.1.0"
  },
  "dependencies": {
    "@octokit/graphql": "^4.3.1",
    "@octokit/rest": "^16.43.1",
    "chalk": "^2.4.2",
    "commander": "^2.19.0",
    "csvtojson": "^2.0.8",
    "fs-extra": "^7.0.1",
    "standard": "^14.3.1"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**.js",
      "src/**/*.js",
      "!<rootDir>/node_modules/",
      "!<rootDir>/src/cli-varys.js",
      "!**/*.test.js"
    ],
    "setupFilesAfterEnv": [
      "<rootDir>/setupTests.js"
    ],
    "coverageReporters": [
      "text-summary",
      "lcov"
    ]
  },
  "standard": {
    "parser": "babel-eslint",
    "ignore": [
      "**/node_modules/**",
      "**/public/**"
    ],
    "envs": [
      "es6",
      "browser",
      "jest"
    ]
  },
  "keywords": [
    "github"
  ]
}
